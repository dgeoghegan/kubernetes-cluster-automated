output "registry_credentials" {
  value = {
    username = local.registry_user
    password = random_password.registry_pass.result
  }
  sensitive = true
}
output "registry_ca_cert_pem" {
  value     = var.ca_cert_pem
  sensitive = false
}

output "registry_server_cert_pem" {
  value     = tls_locally_signed_cert.registry_tls_cert.cert_pem
  sensitive = false
}

output "registry_server_key_pem" {
  value     = tls_private_key.registry_tls_key.private_key_pem
  sensitive = true
}

